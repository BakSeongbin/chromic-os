@-webkit-keyframes blowup {
  from {
    opacity: 1;
  }
  to {
    transform: scale(3);
    opacity: 0;
  }
}

@-webkit-keyframes text-banner-blowup {
  from {
    transform: scale(0.5);
    opacity: 0;
  }
  20% {
    transform: scale(1);
    opacity: 0.75;
  }
  80% {
    transform: scale(1);
    opacity: 0.75;
  }
  to {
    transform: scale(3);
    opacity: 0;
  }
}

